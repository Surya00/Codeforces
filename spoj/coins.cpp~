#include<bits/stdc++.h>
#include<time.h>
using namespace std;
#define ll long long int
double time_taken;
clock_t start, end;	 
int main(int argc, char const *argv[])
{
	ll i,j,k,n,m,s,t,l;

	cin>>t;
    int* poi;
    poi=new int[1000000000];
    memset(poi,-1,1000000000*sizeof(int));
    cout<<*(poi+1000000000-2)<<endl;
	//if(maxv[0]==NULL)
	//	cout<<"hhbff";
	start =  clock();
	
    end= clock();

    time_taken= ((double)(end-start))/CLOCKS_PER_SEC;
    delete [] poi;
    //cout<<time_taken<<" SEC time taken by this code\n";
	return 0;
}